{
  "expo": {
    "scheme": "acme",
    "version": "1.0.0",
    "name": "todostt",
    "slug": "todostt",
    "icon": "./assets/icon.png",
    "plugins": ["expo-router"],
    "web": {
      "bundler": "metro"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#fefeff",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fefeff"
      }
    }
  }
}
